{
    "compileOnSave": false,
    "compilerOptions": {
      "baseUrl": "frontend/src",
      "target": "es5",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": [
        "dom",
        "dom.iterable",
        "es6"
      ],
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "noEmitOnError": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "sourceMap": true,
      "strict": true
    },
    "include": [
        "backEnd/**/*.ts",
        "frontEnd/**/*.ts"
    ],
    "exclude": [
      "node_modules",
      "**/__tests__/**"
    ]
  }
  